{
    "name": "gentritabazi/laradeploy",
    "description": "Laradeploy automate deployment using a GitHub webhook.",
    "homepage": "https://github.com/gentritabazi01/laradeploy",
    "license": "MIT",
    "authors": [
        {
            "name": "Gentrit Abazi",
            "email": "gentritabazi01@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "GentritAbazi\\laradeploy\\": "src/"
        },
        "files" : [
            "src/Helpers/All.php"
        ]
    },
    "require": {
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "GentritAbazi\\Laradeploy\\Providers\\LaradeployServiceProvider"
            ]
        }
    }
}